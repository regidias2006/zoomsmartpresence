{
    "manifest_version": 2,
    "name": "Zoom Smart Presence",
    "version": "1.0.0",
    "web_accessible_resources": ["cpanel.html", "/assets/css/bootstrap.min.css","/assets/js/lib/jquery.min.js", "/assets/js/lib/bootstrap.min.js", "background.css"],
    "description": "This application is for help the FIT and Flex Company monitoring the people in a Meeting in Zoom conference. ",
    "short_name": "Zoom Smart Presence",
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
    "content_scripts": [
      {
        "matches": ["https://flex.zoom.us/*"],
        "all_frames": true,
        "css": ["background.css"],
        "js": ["background.js"],
        "run_at": "document_start"
      }
    ],
    "browser_action": {
      "default_title": "Does a thing when you do a thing",
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/meeting.png",
        "32": "icons/meeting.png"
      }
    }   
   }